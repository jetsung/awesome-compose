DOMAIN=https://vault.example.com # or https://vault.example.com/base-dir

WEBSOCKET_ENABLED=true  # Enable WebSocket notifications.

SMTP_HOST=<smtp.domain.tld>
SMTP_FROM=<vaultwarden@domain.tld>
SMTP_PORT=465 # 465, StartTLS(587)
SMTP_SECURITY=off # 465 force_tls, 587/25 starttls, off
SMTP_USERNAME=<username>
SMTP_PASSWORD=<password>

SIGNUPS_ALLOWED=false # registration disabled

